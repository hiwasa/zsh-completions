#compdef gcof

typeset -A opt_args
local curcontext="$curcontext" context state line

_arguments \
  '1:git branch:->command' \
  ':files:_files' \

# this will be error if used before normal git completion invoked

case $state in
  (command)
    if type __git_branch_names >/dev/null 2>&1;then __git_branch_names;fi
    ;;
esac

return 0
